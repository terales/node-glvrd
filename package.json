  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TerAleS/node-glvrd/issues"
  },
  "homepage": "https://github.com/TerAleS/node-glvrd#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "commitizen": "^2.8.6",
    "coveralls": "^2.11.8",
    "cz-conventional-changelog": "^1.2.0",
    "nock": "^7.7.2",
    "npm-run-all": "^1.5.1",
    "nyc": "^6.0.0",
    "rimraf": "^2.5.2",
    "semantic-release": "^6.3.0",
    "sinon": "^1.17.3",
    "standard": "^8.3.0",
    "snazzy": "5.0.0"
